<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <title>FLIGHTS INDEX</title>
</head>





<body>
  <table>
    <thead>
      <th>Airline</th>
      <th>FlightNo</th>
      <th>Departure</th>
      <th>Details</th>
    </thead>
    <tbody>
      <% flights.forEach(function(f){ %>
        <td><%= f.airline %></td>
        <td><%= f.flightNo %></td>
        <td><%= f.departs %></td>
        <td><a href="/">Details</a></td>
      <% }); %>
    </tbody>
  </table>
  <a href="flights/new">
</body>
</html>